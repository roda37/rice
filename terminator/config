[global_config]
  window_state = maximise
  suppress_multiple_term_dialog = True
[keybindings]
  zoom_in = <Primary>equal
  go_up = <Primary>w
  go_down = <Primary>s
  go_left = <Primary>a
  go_right = <Primary>d
  help = <Alt>asterisk
[profiles]
  [[default]]
    background_darkness = 0.88
    background_type = transparent
    cursor_blink = False
    cursor_color = "#aaaaaa"
    font = Source Code Variable Bold Italic 18
    foreground_color = "#9a9996"
    show_titlebar = False
    scrollbar_position = hidden
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
